   <Row ss:AutoFitHeight="0" ss:Height="25.5">
    <Cell ss:StyleID="s91"><Data ss:Type="String">fort_pickup_A_12</Data></Cell>
    <Cell ss:StyleID="s77"><Data ss:Type="String">$5Дойл: That's it!  The security is broken!  Hurry up Джек, get into the building!</Data></Cell>
    <Cell ss:StyleID="s77"><Data ss:Type="String">$5Дойл: Готово! Система безопасности взломана! Джек, быстрее в здание!</Data></Cell>
    <Cell ss:StyleID="s90"/>
    <Cell ss:Index="7" ss:StyleID="s63"/>
   </Row>
